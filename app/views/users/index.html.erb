<div style="border: black solid;">
    <table style="width: 100%; height: 100%;">
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
        <% @user.each do |e| %>
            <tr>
                <td><%= e.name %></td>
                <td><%= e.email %></td>
                <td><a href="/users/<%= e.id %>">Show</a></td>
                <td><a href="/users/<%= e.id %>/edit">Edit</a></td>
                <td><a href="/users/<%= e.id %>/destroy" data-method="delete">Destroy</a></td>
            </tr>
        <% end %>
    </table>
</div>
<a href="/users/new"><h3>New Dojo</h3></a> 